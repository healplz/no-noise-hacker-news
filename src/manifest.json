{
	"name": "No Noise Hacker News",
	"version": "0.1.0",
	"permissions": [
		"https://news.ycombinator.com/*"
	],
	"content_scripts": [
		{
			"matches": ["https://news.ycombinator.com/*"],
			"js": ["script.js"],
			"run_at": "document_end"
		}
	],
	"manifest_version": 2,
	"icons": { 
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"64": "icon64.png",
		"128": "icon128.png",
		"256": "icon256.png" 
	}
}